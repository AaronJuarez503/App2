<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Estado del Pedido</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            display: flex;
            justify-content: center;
            align-items: center;
           
            margin: 0;
            background-color:  #080202;
        }
        .container {
            background-color: white;
            border-radius: 8px;
            box-shadow: 0 0 10px rgba(0,0,0,0.1);
            padding: 20px;
            text-align: center;
            width: 300px;
        }
        .logo {
            max-width: 200px;
            margin-bottom: 20px;
        }
        .status {
            font-size: 24px;
            font-weight: bold;
            margin-bottom: 20px;
        }
        .truck {
            width: 100px;
            height: auto;
            margin-bottom: 20px;
        }
        .moving {
            animation: move 2s infinite linear;
        }
        @keyframes move {
            0% { transform: translateX(-50px); }
            100% { transform: translateX(50px); }
        }
        .total {
            font-size: 18px;
            font-weight: bold;
            margin-top: 20px;
        }
        .select-status {
            margin-bottom: 20px;
        }
    </style>
</head>
<body>
    <br>
    <div id="container"> 
        <div class="container">
            <img src="${item.marca_imagen}" id="deliveryLogo" data-imagen="${item.marca_imagen}"" data-marca="${item.marca_id}" data-fecha ="${item.fecha_hora_pedido}" class="logo" id="deliveryLogo">
            <select id="statusSelect" class="select-status">
                <option value="en-camino">En camino</option>
                <option value="entregado">Entregado</option>
                <option value="no-entregado">${no entregado} </option>
            </select>
            <div class="status" id="status">${item.estados} </div>
            <img src="/stylesheets/Delivery Animation.gif" style="height: 180px;" alt="Camión de entrega">
            <div class="total" id="total" >Total: $0.00</div>
        </div>

    </div>
        
 
  
    


    <script>
        const statusSelect = document.getElementById('statusSelect');
        const status = document.getElementById('status');
        const total = document.getElementById('total');
        const deliveryLogo = document.getElementById('deliveryLogo');

        const deliveryCompanies = [
            { name: 'Envíos Rápidos', logo: 'data:image/png;base64,' },
            { name: 'Express Delivery', logo: 'data:image/png;base64,' },
            { name: 'Mega Transportes', logo: 'data:image/png;base64,' }
        ];


        function updateStatus() {
            const selectedStatus = statusSelect.value;

            switch(selectedStatus) {
                case 'en-camino':
                    status.textContent = 'En camino';
                    status.style.color = 'blue';
                    truck.style.display = 'inline';
                    truck.classList.add('moving');
                    break;
                case 'entregado':
                    status.textContent = 'Entregado';
                    status.style.color = 'green';
                    truck.style.display = 'none';
                    break;
                case 'no-entregado':
                    status.textContent = 'No entregado';
                    status.style.color = 'red';
                    truck.style.display = 'none';
                    break;
            }
        }

        function updateTotal() {
            const randomTotal = (Math.random() * 100 + 50).toFixed(2);
            total.textContent = `Total: $${randomTotal}`;
        }

        function updateDeliveryCompany() {
            const randomCompany = deliveryCompanies[Math.floor(Math.random() * deliveryCompanies.length)];
            deliveryLogo.src = randomCompany.logo;
            deliveryLogo.alt = `Logo de ${randomCompany.name}`;
        }

        statusSelect.addEventListener('change', updateStatus);

        // Inicializar la interfaz
       // updateStatus();
        updateTotal();
        updateDeliveryCompany();
    </script>
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <script src="/javascript/funciones2.js"></script>
    <script src="/javascript/main2.js"></script>
    



</body>
</html>